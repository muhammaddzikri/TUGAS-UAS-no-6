public class Main
{
    public static void main(String[] args) {

        int row;
        int maks_row = 19;
        int star;
        for(row=1; row< maks_row; row++) {
            for(star=1; star<=row; star++) {
                System.out.print("7");
            }
            System.out.println();
        }
    }
}
